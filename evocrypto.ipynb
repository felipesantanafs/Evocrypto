    "from binance.client import Client\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import matplotlib.dates as mdates\n",
    "import matplotlib.ticker as mticker\n",
    "import os\n",
    "\n",
    "# =======================================\n",
    "# 1. CONFIGURAÇÕES GERAIS\n",
    "# =======================================\n",
    "\n",
    "client = Client(api_key='', api_secret='')\n",
    "\n",
    "ALTCOINS = ['ETH', 'XRP', 'LTC', 'BCH', 'BNB',\n",
    "            'TRX', 'ADA', 'XLM', 'XMR', 'EOS']  \n",
    "QUOTE = 'USDT'\n",
    "BTC_SYMBOL = 'BTCUSDT'\n",
    "\n",
    "interval = Client.KLINE_INTERVAL_1DAY\n",
    "start_str = '2020-10-01'   \n",
    "end_str   = '2025-10-31'\n",
    "\n",
    "output_dir = 'fechamentos'\n",
    "os.makedirs(output_dir, exist_ok=True)\n",
    "\n",
    "\n",
    "# =======================================\n",
    "# 2. FUNÇÃO PARA BAIXAR KLINES\n",
    "# =======================================\n",
    "\n",
    "def get_klines_df(symbol, interval, start_str, end_str):\n",
    "    klines = client.get_historical_klines(symbol, interval, start_str, end_str)\n",
    "\n",
    "    if not klines:\n",
    "        raise ValueError(f\"Nenhum dado retornado para {symbol}.\")\n",
    "\n",
    "    columns = [\n",
    "        'open_time', 'open', 'high', 'low', 'close', 'volume',\n",
    "        'close_time', 'quote_asset_volume', 'number_of_trades',\n",
    "        'taker_buy_base_asset_volume', 'taker_buy_quote_asset_volume',\n",
    "        'ignore'\n",
    "    ]\n",
    "\n",
    "    df = pd.DataFrame(klines, columns=columns)\n",
    "    df['open_time'] = pd.to_datetime(df['open_time'], unit='ms')\n",
    "\n",
    "    for col in ['open', 'high', 'low', 'close', 'volume']:\n",
    "        df[col] = df[col].astype(float)\n",
    "\n",
    "    df['symbol'] = symbol\n",
    "    return df\n",
    "\n",
    "\n",
    "# =======================================\n",
    "# 3. BAIXAR DADOS\n",
    "# =======================================\n",
    "\n",
    "altcoin_dfs = []\n",
    "\n",
    "for base in ALTCOINS:\n",
    "    symbol = base + QUOTE\n",
    "    print(f\"Baixando dados de {symbol}...\")\n",
    "    try:\n",
    "        df_sym = get_klines_df(symbol, interval, start_str, end_str)\n",
    "        altcoin_dfs.append(df_sym)\n",
    "\n",
    "        csv_path = os.path.join(output_dir, f'{symbol}_{interval}_data.csv')\n",
    "        df_sym.to_csv(csv_path, index=False)\n",
    "        print(f\"  -> salvo em {csv_path}\")\n",
    "    except Exception as e:\n",
    "        print(f\"  ERRO ao baixar {symbol}: {e}\")\n",
    "\n",
    "if altcoin_dfs:\n",
    "    df_altcoins = pd.concat(altcoin_dfs, ignore_index=True)\n",
    "    print(\"\\nAltcoins - primeiras linhas:\")\n",
    "    print(df_altcoins[['open_time', 'symbol', 'close']].head())\n",
    "else:\n",
    "    df_altcoins = pd.DataFrame()\n",
    "    print(\"\\nNenhuma altcoin foi baixada.\")\n",
    "\n",
    "print(f\"\\nBaixando dados de {BTC_SYMBOL}...\")\n",
    "df_btc = get_klines_df(BTC_SYMBOL, interval, start_str, end_str)\n",
    "csv_btc_path = os.path.join(output_dir, f'{BTC_SYMBOL}_{interval}_data.csv')\n",
    "df_btc.to_csv(csv_btc_path, index=False)\n",
    "print(f\"  -> BTC salvo em {csv_btc_path}\")\n",
    "print(\"\\nBTC - primeiras linhas:\")\n",
    "print(df_btc[['open_time', 'close']].head())\n",
    "\n",
    "# OBS.: BTC **não** entra mais no universo investível, só benchmark.\n",
    "if not df_altcoins.empty:\n",
    "    print(\"\\nUniverso final (ALTCOINS APENAS) - primeiras linhas:\")\n",
    "    print(df_altcoins[['open_time', 'symbol', 'close']].head())\n",
    "else:\n",
    "    # fallback: se não tiver altcoin nenhuma, usa BTC só para não quebrar\n",
    "    df_altcoins = df_btc.copy()\n",
    "    df_altcoins['symbol'] = BTC_SYMBOL\n",
    "    print(\"\\nFallback: universo virou apenas BTC (não ideal).\")\n",
    "\n",
    "# gráfico simples do BTC\n",
    "plt.figure(figsize=(10, 4))\n",
    "plt.plot(df_btc['open_time'], df_btc['close'], label='BTCUSDT')\n",
    "plt.title('Preço diário do Bitcoin (BTCUSDT)')\n",
    "plt.xlabel('Tempo')\n",
    "plt.ylabel('Preço (USDT)')\n",
    "plt.grid(True, linestyle='--', alpha=0.4)\n",
    "plt.legend()\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "\n",
    "# ======================================================\n",
    "# 4. MARKETDATA: ORGANIZA PAINEL\n",
    "# ======================================================\n",
    "\n",
    "class MarketData:\n",
    "    def __init__(self, df_altcoins, df_btc):\n",
    "        # df_altcoins contém APENAS altcoins (BTC só como benchmark)\n",
    "        self.prices_alt = df_altcoins.pivot(index='open_time',\n",
    "                                            columns='symbol',\n",
    "                                            values='close').sort_index()\n",
    "        self.volumes_alt = df_altcoins.pivot(index='open_time',\n",
    "                                             columns='symbol',\n",
    "                                             values='volume').sort_index()\n",
    "        self.price_btc = df_btc.set_index('open_time')['close'].sort_index()\n",
    "\n",
    "        # alinhar datas\n",
    "        common_idx = self.prices_alt.index.intersection(self.price_btc.index)\n",
    "        self.prices_alt = self.prices_alt.loc[common_idx]\n",
    "        self.volumes_alt = self.volumes_alt.loc[common_idx]\n",
    "        self.price_btc = self.price_btc.loc[common_idx]\n",
    "\n",
    "        # retornos diários (não mata linhas parcialmente vazias)\n",
    "        self.rets_alt = self.prices_alt.pct_change(fill_method=None)\n",
    "        self.rets_alt = self.rets_alt.dropna(how='all')\n",
    "        self.rets_btc = self.price_btc.pct_change(fill_method=None)\n",
    "        self.rets_btc = self.rets_btc.reindex(self.rets_alt.index)\n",
    "\n",
    "        print(\"\\n[MarketData] formas:\")\n",
    "        print(\" prices_alt:\", self.prices_alt.shape)\n",
    "        print(\" rets_alt  :\", self.rets_alt.shape)\n",
    "        print(\" rets_btc  :\", self.rets_btc.shape)\n",
    "\n",
    "\n",
    "# ======================================================\n",
    "# 5. FEATURE ENGINE (M, V, C, L) - SEM LOOKAHEAD\n",
    "# ======================================================\n",
    "\n",
    "class FeatureEngine:\n",
    "    def __init__(self, mkt: MarketData,\n",
    "                 win_mom=30, win_vol=30, win_corr=60, win_liq=30):\n",
    "        self.mkt = mkt\n",
    "        self.win_mom = win_mom\n",
    "        self.win_vol = win_vol\n",
    "        self.win_corr = win_corr\n",
    "        self.win_liq = win_liq\n",
    "        self._build_features()\n",
    "\n",
    "    def _build_features(self):\n",
    "        prices = self.mkt.prices_alt\n",
    "        rets_alt = self.mkt.rets_alt\n",
    "        rets_btc = self.mkt.rets_btc\n",
    "        vols = self.mkt.volumes_alt\n",
    "\n",
    "        # Momentum: preço atual / preço de t-30 - 1\n",
    "        M_raw = prices / prices.shift(self.win_mom) - 1\n",
    "\n",
    "        # Vol 30d dos retornos\n",
    "        V_raw = rets_alt.rolling(self.win_vol).std()\n",
    "\n",
    "        # Correlação 60d com BTC\n",
    "        C_dict = {}\n",
    "        for col in rets_alt.columns:\n",
    "            C_dict[col] = rets_alt[col].rolling(self.win_corr).corr(rets_btc)\n",
    "        C_raw = pd.DataFrame(C_dict)\n",
    "\n",
    "        # Liquidez: média 30d do volume (log)\n",
    "        liq_raw = vols.rolling(self.win_liq).mean()\n",
    "        L_raw = np.log1p(liq_raw)\n",
    "\n",
    "        idx_full = rets_alt.index\n",
    "        max_win = max(self.win_mom, self.win_vol, self.win_corr, self.win_liq)\n",
    "\n",
    "        # Usaremos features de t-1 para prever retorno de t\n",
    "        idx = idx_full[max_win + 1:]\n",
    "\n",
    "        self.M = M_raw.shift(1).loc[idx]\n",
    "        self.V = V_raw.shift(1).loc[idx]\n",
    "        self.C = C_raw.shift(1).loc[idx]\n",
    "        self.L = L_raw.shift(1).loc[idx]\n",
    "\n",
    "        # Retornos (sem shift): retorno de t\n",
    "        self.rets_alt = rets_alt.loc[idx]\n",
    "        self.rets_btc = rets_btc.loc[idx]\n",
    "\n",
    "        print(\"\\n[FeatureEngine] formas (depois do corte inicial):\")\n",
    "        print(\" index datas:\", len(idx))\n",
    "        print(\" M:\", self.M.shape, \"V:\", self.V.shape,\n",
    "              \"C:\", self.C.shape, \"L:\", self.L.shape)\n",
    "\n",
    "    @staticmethod\n",
    "    def _zscore_cross_section(row: pd.Series):\n",
    "        mu = row.mean()\n",
    "        sigma = row.std()\n",
    "        if sigma == 0 or np.isnan(sigma):\n",
    "            return row * 0.0\n",
    "        return (row - mu) / sigma\n",
    "\n",
    "    def get_features_on_date(self, date):\n",
    "        M_row = self.M.loc[date]\n",
    "        V_row = self.V.loc[date]\n",
    "        C_row = self.C.loc[date]\n",
    "        L_row = self.L.loc[date]\n",
    "\n",
    "        M_z = self._zscore_cross_section(M_row)\n",
    "        V_z = self._zscore_cross_section(V_row)\n",
    "        C_z = self._zscore_cross_section(C_row)\n",
    "        L_z = self._zscore_cross_section(L_row)\n",
    "\n",
    "        return M_z, V_z, C_z, L_z\n",
    "\n",
    "\n",
    "# ======================================================\n",
    "# 6. CROMOSSOMO (AGORA COM c_star + CAPS DE PESO)\n",
    "# ======================================================\n",
    "\n",
    "class Chromosome:\n",
    "    def __init__(self, genes):\n",
    "        # genes = [alpha1, alpha2, alpha3, alpha4, p, c_star]\n",
    "        self.alpha1, self.alpha2, self.alpha3, self.alpha4, self.p, self.c_star = genes\n",
    "\n",
    "    def compute_weights(self, M, V, C, L):\n",
    "        # c_star é alvo de correlação (entre 0 e 1)\n",
    "        S = (self.alpha1 * M\n",
    "             - self.alpha2 * V\n",
    "             - self.alpha3 * (C - self.c_star).abs()\n",
    "             + self.alpha4 * L)\n",
    "\n",
    "        S_pos = np.maximum(S, 0.0)\n",
    "\n",
    "        if S_pos.sum() <= 0 or np.all(np.isclose(S_pos.values, 0.0)):\n",
    "            return S_pos * 0.0\n",
    "\n",
    "        w = S_pos ** self.p\n",
    "        w = w / w.sum()\n",
    "\n",
    "        # HARD CAP de peso por ativo (ex.: máximo 20% por altcoin)\n",
    "        max_weight = 0.20\n",
    "        w = w.clip(upper=max_weight)\n",
    "        if w.sum() > 0:\n",
    "            w = w / w.sum()\n",
    "\n",
    "        return w\n",
    "# ======================================================\n",
    "# 7. BACKTESTER + FITNESS (métricas comparativas)\n",
    "# ======================================================\n",
    "\n",
    "class BacktesterGA:\n",
    "    def __init__(self, feat: FeatureEngine,\n",
    "                 lambda_sharpe=1.0,     # peso no Sharpe relativo\n",
    "                 lambda_vol=5.0,        # penalização da vol relativa\n",
    "                 lambda_turnover=0.5,   # penalização do turnover\n",
    "                 trading_cost=0.0015,   # custo por unidade de turnover\n",
    "                 epsilon=1e-6,\n",
    "                 lambda_gap=2.0,        # penaliza gap ergódico\n",
    "                 lambda_conc=3.0,       # penaliza concentração\n",
    "                 conc_target=0.15,      # alvo de diversificação\n",
    "                 lambda_skew=1.0,       # penaliza skew negativa\n",
    "                 target_vol_ann=0.25,   # alvo de vol anualizada\n",
    "                 vol_window=60          # janela para realized vol\n",
    "                 ):\n",
    "        self.feat = feat\n",
    "        self.lambda_sharpe = lambda_sharpe\n",
    "        self.lambda_vol = lambda_vol\n",
    "        self.lambda_turnover = lambda_turnover\n",
    "        self.trading_cost = trading_cost\n",
    "        self.epsilon = epsilon\n",
    "        self.lambda_gap = lambda_gap\n",
    "        self.lambda_conc = lambda_conc\n",
    "        self.conc_target = conc_target\n",
    "        self.lambda_skew = lambda_skew\n",
    "        self.target_vol_ann = target_vol_ann\n",
    "        self.vol_window = vol_window\n",
    "\n",
    "    def run_backtest(self, chromosome: Chromosome,\n",
    "                     start_date, end_date):\n",
    "        idx = self.feat.rets_alt.loc[start_date:end_date].index\n",
    "        if len(idx) == 0:\n",
    "            raise ValueError(\"Período de backtest vazio – verifique datas.\")\n",
    "\n",
    "        rets_alt = self.feat.rets_alt.loc[idx]\n",
    "        rets_btc = self.feat.rets_btc.loc[idx]\n",
    "\n",
    "        symbols = rets_alt.columns\n",
    "        last_w = pd.Series(0.0, index=symbols)\n",
    "\n",
    "        # Armazenamento de séries temporais\n",
    "        port_rets_list = []\n",
    "        btc_rets_list = []\n",
    "        turnover_list = []\n",
    "        conc_list = []\n",
    "        weights_dict = {}\n",
    "\n",
    "        for date in idx:\n",
    "            M, V, C, L = self.feat.get_features_on_date(date)\n",
    "            M = M.reindex(symbols)\n",
    "            V = V.reindex(symbols)\n",
    "            C = C.reindex(symbols)\n",
    "            L = L.reindex(symbols)\n",
    "\n",
    "            w = chromosome.compute_weights(M, V, C, L)\n",
    "            w = w.fillna(0.0)\n",
    "            if w.sum() > 0:\n",
    "                w = w / w.sum()\n",
    "\n",
    "            # Concentração\n",
    "            conc = float((w ** 2).sum())\n",
    "            conc_list.append(conc)\n",
    "\n",
    "            # Turnover\n",
    "            turnover = 0.5 * np.abs(w - last_w).sum()\n",
    "            turnover_list.append(turnover)\n",
    "            last_w = w\n",
    "            weights_dict[date] = w.copy()\n",
    "\n",
    "            # Retorno do portfólio (com custo)\n",
    "            r_alt = rets_alt.loc[date]\n",
    "            r_portfolio = (w * r_alt).sum() - self.trading_cost * turnover\n",
    "\n",
    "            # Volatility targeting\n",
    "            if len(port_rets_list) >= self.vol_window:\n",
    "                realized_vol_daily = np.nanstd(port_rets_list[-self.vol_window:], ddof=1)\n",
    "                realized_vol_ann = realized_vol_daily * np.sqrt(252)\n",
    "                if realized_vol_ann > 0:\n",
    "                    scale = self.target_vol_ann / (realized_vol_ann + self.epsilon)\n",
    "                    scale = np.clip(scale, 0.0, 2.0)\n",
    "                    r_portfolio = r_portfolio * scale\n",
    "\n",
    "            port_rets_list.append(r_portfolio)\n",
    "            btc_rets_list.append(rets_btc.loc[date])\n",
    "\n",
    "        port_rets = np.array(port_rets_list)\n",
    "        btc_rets = np.array(btc_rets_list)\n",
    "        turnover_arr = np.array(turnover_list)\n",
    "        conc_arr = np.array(conc_list)\n",
    "        weights_df = pd.DataFrame.from_dict(weights_dict, orient='index')\n",
    "        \n",
    "        # ===== MÉTRICAS COMPARATIVAS =====\n",
    "        ann_factor = 252\n",
    "        \n",
    "        # Retornos anualizados\n",
    "        mu_port_ann = np.nanmean(port_rets) * ann_factor\n",
    "        mu_btc_ann = np.nanmean(btc_rets) * ann_factor\n",
    "        \n",
    "        # Volatilidades anualizadas\n",
    "        sigma_port_ann = np.nanstd(port_rets, ddof=1) * np.sqrt(ann_factor)\n",
    "        sigma_btc_ann = np.nanstd(btc_rets, ddof=1) * np.sqrt(ann_factor)\n",
    "        \n",
    "        # SHARPE LINEAR\n",
    "        sharpe_port = mu_port_ann / (sigma_port_ann + self.epsilon)\n",
    "        sharpe_btc = mu_btc_ann / (sigma_btc_ann + self.epsilon)\n",
    "        \n",
    "        # Retornos ergódicos (log)\n",
    "        log_port = np.log1p(port_rets)\n",
    "        log_btc = np.log1p(btc_rets)\n",
    "        mu_port_erg = np.nanmean(log_port) * ann_factor\n",
    "        mu_btc_erg = np.nanmean(log_btc) * ann_factor\n",
    "        sigma_port_erg = np.nanstd(log_port, ddof=1) * np.sqrt(ann_factor)\n",
    "        sigma_btc_erg = np.nanstd(log_btc, ddof=1) * np.sqrt(ann_factor)\n",
    "        sharpe_port_erg = mu_port_erg / (sigma_port_erg + self.epsilon)\n",
    "        sharpe_btc_erg = mu_btc_erg / (sigma_btc_erg + self.epsilon)\n",
    "        \n",
    "        # Métricas relativas\n",
    "        ret_rel = mu_port_ann - mu_btc_ann\n",
    "        vol_rel = sigma_port_ann - sigma_btc_ann\n",
    "        sharpe_rel = sharpe_port - sharpe_btc\n",
    "        \n",
    "        # Tracking error e Information Ratio\n",
    "        ret_diff = port_rets - btc_rets\n",
    "        tracking_error = np.nanstd(ret_diff, ddof=1) * np.sqrt(ann_factor)\n",
    "        info_ratio = np.nanmean(ret_diff) / (np.nanstd(ret_diff, ddof=1) + self.epsilon) * np.sqrt(ann_factor)\n",
    "        \n",
    "        # Beta e Alpha\n",
    "        covariance = np.nanmean((port_rets - np.nanmean(port_rets)) * (btc_rets - np.nanmean(btc_rets)))\n",
    "        beta = covariance / (np.nanvar(btc_rets, ddof=1) + self.epsilon)\n",
    "        alpha = mu_port_ann - beta * mu_btc_ann\n",
    "        \n",
    "        # Correlação\n",
    "        if sigma_port_ann > 0 and sigma_btc_ann > 0:\n",
    "            corr_pb = np.corrcoef(port_rets, btc_rets)[0, 1]\n",
    "        else:\n",
    "            corr_pb = 0.0\n",
    "        \n",
    "        # Max Drawdown\n",
    "        eq_curve = (1 + port_rets).cumprod()\n",
    "        peak = np.maximum.accumulate(eq_curve)\n",
    "        dd = (eq_curve / peak) - 1.0\n",
    "        max_dd = np.nanmin(dd)\n",
    "        \n",
    "        # Skewness e Kurtosis\n",
    "        skew_port = pd.Series(port_rets).skew()\n",
    "        kurt_port = pd.Series(port_rets).kurtosis()\n",
    "        skew_btc = pd.Series(btc_rets).skew()\n",
    "        kurt_btc = pd.Series(btc_rets).kurtosis()\n",
    "        \n",
    "        # Turnover e concentração\n",
    "        avg_turnover = np.nanmean(turnover_arr)\n",
    "        avg_conc = np.nanmean(conc_arr)\n",
    "        conc_excess = max(0.0, avg_conc - self.conc_target)\n",
    "        \n",
    "        # Gap ergódico\n",
    "        erg_gap_port = max(0.0, mu_port_ann - mu_port_erg)\n",
    "        erg_gap_btc = max(0.0, mu_btc_ann - mu_btc_erg)\n",
    "        erg_gap_rel = erg_gap_port - erg_gap_btc\n",
    "        \n",
    "        # FITNESS\n",
    "        fitness = (\n",
    "            ret_rel +                                      # retorno relativo\n",
    "            self.lambda_sharpe * sharpe_rel -              # Sharpe relativo\n",
    "            self.lambda_vol * max(0.0, vol_rel) -          # penaliza vol maior que BTC\n",
    "            self.lambda_turnover * avg_turnover -          # penaliza turnover\n",
    "            self.lambda_gap * erg_gap_rel -                # penaliza gap ergódico\n",
    "            self.lambda_conc * conc_excess -               # penaliza concentração\n",
    "            self.lambda_skew * max(0.0, -skew_port)        # penaliza skew negativa\n",
    "        )\n",
    "\n",
    "        # ===== DATAFRAMES PARA ANÁLISE =====\n",
    "        \n",
    "        # 1. Série temporal de retornos e métricas\n",
    "        series_df = pd.DataFrame({\n",
    "            'date': idx,\n",
    "            'port_rets': port_rets,\n",
    "            'btc_rets': btc_rets,\n",
    "            'ret_diff': ret_diff,\n",
    "            'turnover': turnover_arr,\n",
    "            'concentration': conc_arr,\n",
    "            'eq_curve_port': eq_curve,\n",
    "            'drawdown': dd\n",
    "        })\n",
    "        series_df.set_index('date', inplace=True)\n",
    "        \n",
    "        # Adicionar pesos ao series_df\n",
    "        if not weights_df.empty:\n",
    "            series_df = series_df.join(weights_df, how='left')\n",
    "        \n",
    "        # 2. DataFrame de métricas comparativas\n",
    "        metrics_df = pd.DataFrame({\n",
    "            'Portfólio': [mu_port_ann, sigma_port_ann, sharpe_port, mu_port_erg, sigma_port_erg, sharpe_port_erg,\n",
    "                         max_dd, skew_port, kurt_port, avg_turnover, avg_conc, beta, alpha],\n",
    "            'Bitcoin': [mu_btc_ann, sigma_btc_ann, sharpe_btc, mu_btc_erg, sigma_btc_erg, sharpe_btc_erg,\n",
    "                       np.nan, skew_btc, kurt_btc, np.nan, np.nan, 1.0, 0.0],\n",
    "            'Relativo/Diferença': [ret_rel, vol_rel, sharpe_rel, np.nan, np.nan, np.nan,\n",
    "                                  np.nan, skew_port - skew_btc, kurt_port - kurt_btc, avg_turnover, conc_excess, np.nan, np.nan]\n",
    "        }, index=['Retorno Anualizado', 'Volatilidade Anualizada', 'Sharpe', \n",
    "                  'Retorno Ergódico', 'Vol Ergódica', 'Sharpe Ergódico',\n",
    "                  'Max Drawdown', 'Skewness', 'Kurtosis', 'Turnover Médio', \n",
    "                  'Excesso de Concentração', 'Beta', 'Alpha'])\n",
    "        \n",
    "        # 3. Métricas de risco adicionais\n",
    "        risk_metrics_df = pd.DataFrame({\n",
    "            'valor': [info_ratio, tracking_error, corr_pb, erg_gap_rel, sharpe_rel]\n",
    "        }, index=['Information Ratio', 'Tracking Error', 'Correlação PB', \n",
    "                  'Gap Ergódico Relativo', 'Sharpe Relativo'])\n",
    "        \n",
    "        results = {\n",
    "            \"series_df\": series_df,\n",
    "            \"metrics_df\": metrics_df,\n",
    "            \"risk_metrics_df\": risk_metrics_df,\n",
    "            \"chromosome\": chromosome,\n",
    "            \"fitness\": fitness\n",
    "        }\n",
    "        \n",
    "        return results\n",
    "\n",
    "\n",
    "# ======================================================\n",
    "# 8. GENETIC ALGORITHM\n",
    "# ======================================================\n",
    "\n",
    "class GeneticAlgorithm:\n",
    "    def __init__(self, backtester: BacktesterGA,\n",
    "                 train_mask,\n",
    "                 pop_size=30,\n",
    "                 n_generations=40,\n",
    "                 cx_prob=0.8,\n",
    "                 mut_prob=0.2,\n",
    "                 param_ranges=None,\n",
    "                 n_folds=3,\n",
    "                 reg_lambda=0.01,\n",
    "                 random_state=None):\n",
    "        self.bt = backtester\n",
    "        self.pop_size = pop_size\n",
    "        self.n_generations = n_generations\n",
    "        self.cx_prob = cx_prob\n",
    "        self.mut_prob = mut_prob\n",
    "        self.rng = np.random.default_rng(random_state)\n",
    "        self.n_folds = n_folds\n",
    "        self.reg_lambda = reg_lambda\n",
    "\n",
    "        if param_ranges is None:\n",
    "            param_ranges = [\n",
    "                (0.0, 4.0),  # alpha1\n",
    "                (0.0, 4.0),  # alpha2\n",
    "                (0.0, 4.0),  # alpha3\n",
    "                (0.0, 4.0),  # alpha4\n",
    "                (0.5, 3.0),  # p\n",
    "                (0.0, 1.0),  # c_star\n",
    "            ]\n",
    "        self.param_ranges = param_ranges\n",
    "\n",
    "        all_dates = self.bt.feat.rets_alt.index\n",
    "        if len(train_mask) != len(all_dates):\n",
    "            raise ValueError(\"train_mask deve ter o mesmo tamanho do índice de rets_alt.\")\n",
    "\n",
    "        self.train_idx = all_dates[train_mask]\n",
    "        self.train_start = self.train_idx.min()\n",
    "        self.train_end = self.train_idx.max()\n",
    "\n",
    "        folds = np.array_split(self.train_idx, self.n_folds)\n",
    "        self.cv_windows = []\n",
    "        for f in folds:\n",
    "            if len(f) == 0:\n",
    "                continue\n",
    "            self.cv_windows.append((f.min(), f.max()))\n",
    "\n",
    "    def _sample_gene(self, low, high):\n",
    "        return self.rng.uniform(low, high)\n",
    "\n",
    "    def _init_population(self):\n",
    "        pop = []\n",
    "        for _ in range(self.pop_size):\n",
    "            genes = [self._sample_gene(lo, hi) for (lo, hi) in self.param_ranges]\n",
    "            pop.append(np.array(genes, dtype=float))\n",
    "        return pop\n",
    "\n",
    "    def _evaluate(self, genes):\n",
    "        chrom = Chromosome(genes)\n",
    "        fold_fits = []\n",
    "\n",
    "        for (start, end) in self.cv_windows:\n",
    "            res = self.bt.run_backtest(chrom, start, end)\n",
    "            fold_fits.append(res[\"fitness\"])\n",
    "\n",
    "        if len(fold_fits) == 0:\n",
    "            return -np.inf, None\n",
    "\n",
    "        avg_fit = float(np.mean(fold_fits))\n",
    "        reg_penalty = self.reg_lambda * float(np.sum(genes ** 2))\n",
    "        return avg_fit - reg_penalty, None\n",
    "\n",
    "    def _tournament_select(self, population, fitnesses, k=3):\n",
    "        idxs = self.rng.integers(0, len(population), size=k)\n",
    "        best = idxs[0]\n",
    "        for i in idxs[1:]:\n",
    "            if fitnesses[i] > fitnesses[best]:\n",
    "                best = i\n",
    "        return population[best].copy()\n",
    "\n",
    "    def _crossover(self, parent1, parent2):\n",
    "        alpha = self.rng.uniform(0.0, 1.0, size=parent1.shape)\n",
    "        child1 = alpha * parent1 + (1 - alpha) * parent2\n",
    "        child2 = alpha * parent2 + (1 - alpha) * parent1\n",
    "        return child1, child2\n",
    "\n",
    "    def _mutate(self, genes, mut_scale=0.1):\n",
    "        for i, (lo, hi) in enumerate(self.param_ranges):\n",
    "            if self.rng.random() < self.mut_prob:\n",
    "                span = hi - lo\n",
    "                noise = self.rng.normal(0, mut_scale * span)\n",
    "                genes[i] = float(np.clip(genes[i] + noise, lo, hi))\n",
    "        return genes\n",
    "\n",
    "    def run(self):\n",
    "        population = self._init_population()\n",
    "        fitnesses = []\n",
    "        for g in population:\n",
    "            f, _ = self._evaluate(g)\n",
    "            fitnesses.append(f)\n",
    "        fitnesses = np.array(fitnesses)\n",
    "\n",
    "        best_idx = int(np.argmax(fitnesses))\n",
    "        best_genes = population[best_idx].copy()\n",
    "        best_fit = float(fitnesses[best_idx])\n",
    "\n",
    "        print(f\"Geração 0 | melhor fitness (CV) = {best_fit:.4f}\")\n",
    "\n",
    "        for gen in range(1, self.n_generations + 1):\n",
    "            new_pop = []\n",
    "            elite = best_genes.copy()\n",
    "            new_pop.append(elite)\n",
    "\n",
    "            while len(new_pop) < self.pop_size:\n",
    "                p1 = self._tournament_select(population, fitnesses)\n",
    "                p2 = self._tournament_select(population, fitnesses)\n",
    "\n",
    "                if self.rng.random() < self.cx_prob:\n",
    "                    c1, c2 = self._crossover(p1, p2)\n",
    "                else:\n",
    "                    c1, c2 = p1.copy(), p2.copy()\n",
    "\n",
    "                c1 = self._mutate(c1)\n",
    "                c2 = self._mutate(c2)\n",
    "\n",
    "                new_pop.append(c1)\n",
    "                if len(new_pop) < self.pop_size:\n",
    "                    new_pop.append(c2)\n",
    "\n",
    "            population = new_pop\n",
    "            fitnesses = []\n",
    "            for g in population:\n",
    "                f, _ = self._evaluate(g)\n",
    "                fitnesses.append(f)\n",
    "            fitnesses = np.array(fitnesses)\n",
    "\n",
    "            gen_best_idx = int(np.argmax(fitnesses))\n",
    "            gen_best_fit = float(fitnesses[gen_best_idx])\n",
    "            gen_best_genes = population[gen_best_idx].copy()\n",
    "\n",
    "            if gen_best_fit > best_fit:\n",
    "                best_fit = gen_best_fit\n",
    "                best_genes = gen_best_genes\n",
    "\n",
    "            print(f\"Geração {gen} | melhor fitness (CV) = {best_fit:.4f}\")\n",
    "\n",
    "        best_chrom = Chromosome(best_genes)\n",
    "        full_train_results = self.bt.run_backtest(best_chrom,\n",
    "                                                  self.train_start,\n",
    "                                                  self.train_end)\n",
    "\n",
    "        return best_chrom, full_train_results\n",
    "\n",
    "\n",
    "# ======================================================\n",
    "# 9. RODAR: IN-SAMPLE / OUT-OF-SAMPLE\n",
    "# ======================================================\n",
    "\n",
    "mkt = MarketData(df_altcoins, df_btc)\n",
    "feat_engine = FeatureEngine(mkt)\n",
    "\n",
    "all_dates = feat_engine.rets_alt.index.sort_values()\n",
    "if len(all_dates) == 0:\n",
    "    raise ValueError(\"Nenhuma data com features válidas.\")\n",
    "\n",
    "start_feat = all_dates.min()\n",
    "split_date = start_feat + pd.DateOffset(years=4)\n",
    "\n",
    "train_mask = all_dates < split_date\n",
    "test_mask = all_dates >= split_date\n",
    "\n",
    "print(\"\\n[Split]\")\n",
    "print(f\"  Início: {start_feat}\")\n",
    "print(f\"  Fim treino: {split_date}\")\n",
    "print(f\"  Última data: {all_dates.max()}\")\n",
    "print(f\"  Dias treino: {int(train_mask.sum())} | Dias teste: {int(test_mask.sum())}\")\n",
    "\n",
    "backtester = BacktesterGA(\n",
    "    feat_engine,\n",
    "    lambda_sharpe=1.0,\n",
    "    lambda_vol=5.0,\n",
    "    lambda_turnover=0.5,\n",
    "    trading_cost=0.0015,\n",
    "    lambda_gap=2.0,\n",
    "    lambda_conc=3.0,\n",
    "    conc_target=0.15,\n",
    "    lambda_skew=1.0,\n",
    "    target_vol_ann=0.25,\n",
    "    vol_window=60\n",
    ")\n",
    "\n",
    "ga = GeneticAlgorithm(backtester,\n",
    "                      train_mask=train_mask,\n",
    "                      pop_size=30,\n",
    "                      n_generations=40,\n",
    "                      n_folds=3,\n",
    "                      reg_lambda=0.01,\n",
    "                      random_state=42)\n",
    "\n",
    "best_chrom, train_results = ga.run()\n",
    "\n",
    "print(\"\\n\" + \"=\"*50)\n",
    "print(\"MELHOR CROMOSSOMO ENCONTRADO:\")\n",
    "print(f\"  alpha1 (Momentum): {best_chrom.alpha1:.4f}\")\n",
    "print(f\"  alpha2 (Vol):      {best_chrom.alpha2:.4f}\")\n",
    "print(f\"  alpha3 (Correl):   {best_chrom.alpha3:.4f}\")\n",
    "print(f\"  alpha4 (Liq):      {best_chrom.alpha4:.4f}\")\n",
    "print(f\"  p (Exponencial):   {best_chrom.p:.4f}\")\n",
    "print(f\"  c* (Alvo Correl):  {best_chrom.c_star:.4f}\")\n",
    "print(f\"Fitness treino:      {train_results['fitness']:.4f}\")\n",
    "print(\"=\"*50)\n",
    "\n",
    "# OUT-OF-SAMPLE\n",
    "test_idx = all_dates[test_mask]\n",
    "test_start = test_idx.min()\n",
    "test_end = test_idx.max()\n",
    "\n",
    "print(f\"\\nRodando out-of-sample: {test_start} a {test_end}\")\n",
    "oos_results = backtester.run_backtest(best_chrom, test_start, test_end)\n",
    "\n",
    "print(\"\\n\" + \"=\"*50)\n",
    "print(\"RESULTADOS OUT-OF-SAMPLE (COMPARATIVOS)\")\n",
    "print(\"=\"*50)\n",
    "\n",
    "# Exibir métricas comparativas\n",
    "print(\"\\nDataFrame de Métricas Principais:\")\n",
    "print(oos_results[\"metrics_df\"].round(4))\n",
    "\n",
    "print(\"\\nDataFrame de Métricas de Risco:\")\n",
    "print(oos_results[\"risk_metrics_df\"].round(4))\n",
    "\n",
    "print(f\"\\nFitness OOS: {oos_results['fitness']:.4f}\")\n",
    "\n",
    "\n",
    "# ======================================================\n",
    "# GRÁFICOS USANDO DATAFRAMES\n",
    "# ======================================================\n",
    "\n",
    "# Acesse os DataFrames\n",
    "series_oos = oos_results[\"series_df\"]\n",
    "metrics_oos = oos_results[\"metrics_df\"]\n",
    "risk_oos = oos_results[\"risk_metrics_df\"]\n",
    "\n",
    "# Gráfico 1: Retorno Acumulado\n",
    "plt.figure(figsize=(12, 5))\n",
    "plt.plot(series_oos.index, series_oos['eq_curve_port'] - 1, \n",
    "         label='Portfólio GA', linewidth=2)\n",
    "plt.plot(series_oos.index, series_oos['btc_rets'].cumsum(), \n",
    "         label='Bitcoin', linewidth=2, alpha=0.7)\n",
    "plt.title('Retorno Acumulado - Out-of-Sample')\n",
    "plt.xlabel('Data')\n",
    "plt.ylabel('Retorno Acumulado')\n",
    "plt.grid(True, linestyle='--', alpha=0.4)\n",
    "plt.legend()\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Gráfico 2: Drawdown\n",
    "plt.figure(figsize=(12, 3))\n",
    "plt.plot(series_oos.index, series_oos['drawdown'] * 100, \n",
    "         color='red', linewidth=1.5)\n",
    "plt.title('Drawdown do Portfólio')\n",
    "plt.xlabel('Data')\n",
    "plt.ylabel('Drawdown (%)')\n",
    "plt.grid(True, linestyle='--', alpha=0.4)\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Gráfico 3: Pesos ao longo do tempo (top 5 ativos)\n",
    "if not series_oos.filter(like='USDT').empty:\n",
    "    top_5_assets = series_oos.filter(like='USDT').iloc[-1].nlargest(5).index\n",
    "    plt.figure(figsize=(12, 5))\n",
    "    plt.stackplot(series_oos.index, \n",
    "                 *[series_oos[asset] for asset in top_5_assets],\n",
    "                 labels=top_5_assets, alpha=0.8)\n",
    "    plt.title('Evolução dos Pesos - Top 5 Ativos')\n",
    "    plt.xlabel('Data')\n",
    "    plt.ylabel('Peso (%)')\n",
    "    plt.legend(loc='upper left')\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "series_oos.to_csv('dados_brutos.csv')"
   ]
