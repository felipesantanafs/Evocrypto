    "# ===============================\n",
    "# 1. Carregar base e preparar índice\n",
    "# ===============================\n",
    "\n",
    "df = pd.read_csv(\"dados_brutos.csv\")\n",
    "\n",
    "df[\"date\"] = pd.to_datetime(df[\"date\"])\n",
    "df = df.sort_values(\"date\").set_index(\"date\")\n",
    "df.index.name = \"Data\"\n",
    "\n",
    "port_rets = df[\"port_rets\"].astype(float)\n",
    "btc_rets  = df[\"btc_rets\"].astype(float)\n",
    "\n",
    "ann_factor = 252\n",
    "window = 60  # ~3 meses de janela rolling\n",
    "\n",
    "# ===============================\n",
    "# 2. Retorno médio (ensemble) vs ergódico (log)\n",
    "# ===============================\n",
    "\n",
    "# médias rolling dos retornos simples (ensemble) – anualizados\n",
    "mu_port_ann_roll = port_rets.rolling(window).mean() * ann_factor\n",
    "mu_btc_ann_roll  = btc_rets.rolling(window).mean() * ann_factor\n",
    "\n",
    "# médias rolling dos retornos log (time-average / ergódico) – anualizados\n",
    "log_port = np.log1p(port_rets)\n",
    "log_btc  = np.log1p(btc_rets)\n",
    "\n",
    "g_port_erg_roll = log_port.rolling(window).mean() * ann_factor\n",
    "g_btc_erg_roll  = log_btc.rolling(window).mean() * ann_factor\n",
    "\n",
    "# ===============================\n",
    "# 3. Gap ergódico (μ_annual − g_ergodic)\n",
    "# ===============================\n",
    "\n",
    "gap_port_roll = mu_port_ann_roll - g_port_erg_roll\n",
    "gap_btc_roll  = mu_btc_ann_roll  - g_btc_erg_roll\n",
    "\n",
    "# valores médios (para usar na legenda / interpretação)\n",
    "mu_port_ann = port_rets.mean() * ann_factor\n",
    "mu_btc_ann  = btc_rets.mean() * ann_factor\n",
    "g_port_erg  = log_port.mean() * ann_factor\n",
    "g_btc_erg   = log_btc.mean() * ann_factor\n",
    "\n",
    "gap_port_mean = mu_port_ann - g_port_erg\n",
    "gap_btc_mean  = mu_btc_ann  - g_btc_erg\n",
    "\n",
    "# ===============================\n",
    "# 4. Gráfico – Ergodicidade\n",
    "# ===============================\n",
    "\n",
    "fig, (ax1, ax2) = plt.subplots(\n",
    "    2, 1,\n",
    "    figsize=(11, 6),\n",
    "    sharex=True,\n",
    "    gridspec_kw={\"height_ratios\": [2, 1]}\n",
    ")\n",
    "\n",
    "# -------- Painel 1: Crescimento médio ergódico (g) vs ensemble --------\n",
    "ax1.plot(\n",
    "    g_port_erg_roll.index,\n",
    "    g_port_erg_roll.values * 100,\n",
    "    linewidth=2,\n",
    "    label=f\"Portfólio – g_erg médio ≈ {g_port_erg*100:.1f}% a.a.\"\n",
    ")\n",
    "\n",
    "ax1.plot(\n",
    "    g_btc_erg_roll.index,\n",
    "    g_btc_erg_roll.values * 100,\n",
    "    linewidth=2,\n",
    "    linestyle=\"--\",\n",
    "    alpha=0.9,\n",
    "    label=f\"Bitcoin – g_erg médio ≈ {g_btc_erg*100:.1f}% a.a.\"\n",
    ")\n",
    "\n",
    "ax1.axhline(0, color=\"black\", linestyle=\":\", linewidth=1)\n",
    "\n",
    "ax1.set_title(\n",
    "    f\"Ergodicidade: Crescimento Médio no Tempo (janela {window} dias, anualizado)\",\n",
    "    fontsize=13\n",
    ")\n",
    "ax1.set_ylabel(\"g_erg (log) – % ao ano\", fontsize=11)\n",
    "ax1.yaxis.set_major_formatter(mticker.PercentFormatter())\n",
    "ax1.grid(True, linestyle=\"--\", alpha=0.4)\n",
    "ax1.legend(loc=\"upper left\", fontsize=9)\n",
    "\n",
    "# -------- Painel 2: Gap ergódico (custo da volatilidade) --------\n",
    "ax2.plot(\n",
    "    gap_port_roll.index,\n",
    "    gap_port_roll.values * 100,\n",
    "    linewidth=1.8,\n",
    "    label=f\"Portfólio – gap médio ≈ {gap_port_mean*100:.1f} p.p. a.a.\"\n",
    ")\n",
    "\n",
    "ax2.plot(\n",
    "    gap_btc_roll.index,\n",
    "    gap_btc_roll.values * 100,\n",
    "    linewidth=1.5,\n",
    "    linestyle=\"--\",\n",
    "    alpha=0.9,\n",
    "    label=f\"Bitcoin – gap médio ≈ {gap_btc_mean*100:.1f} p.p. a.a.\"\n",
    ")\n",
    "\n",
    "ax2.axhline(0, color=\"black\", linewidth=1)\n",
    "ax2.set_title(\"Gap Ergodógico Rolling (μ_anu − g_erg) – penalidade da volatilidade\",\n",
    "              fontsize=12)\n",
    "ax2.set_ylabel(\"Gap ergódico (p.p. ao ano)\", fontsize=11)\n",
    "ax2.yaxis.set_major_formatter(mticker.PercentFormatter())\n",
    "ax2.grid(True, linestyle=\"--\", alpha=0.4)\n",
    "ax2.legend(loc=\"upper left\", fontsize=9)\n",
    "\n",
    "ax2.set_xlabel(\"Data\", fontsize=11)\n",
    "\n",
    "fig.tight_layout(pad=2.0)\n",
    "\n",
    "\n",
    "#fig.savefig(\"grafico_ergodicidade_port_vs_btc.png\", dpi=300, bbox_inches=\"tight\")\n",
    "\n",
    "plt.show()"
   ]
